    "buildJS_CSS": "gulp buildCSS && npm run buildJs",
    "buildCSS": "gulp buildCSS",
    "webpack-dev": "webpack --watch --config webpack.dev.js",
    "webpack": "webpack --config webpack.prod.js",
    "lint": "npm run lint:js",
    "lint:js": "eslint --ext \".js\" --ignore-path .gitignore .",
    "lintfix": "npm run lint:js --fix",
    "prepare": "husky install",
    "prettier": "prettier --write assets/**/*.{js,scss,css} assets/js/scripts/**/*.js assets/js/webpack/assets/**/*.js",
    "updateProjectVersion": "node modules/updateProjectVersion.js"
  },
  "lint-staged": {
    "*.js": "npm run lint:js",
    "*.{js,scss,css}": "npm run prettier"
